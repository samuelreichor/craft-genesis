{
  "name": "samuelreichor/craft-genesis",
  "description": "Bulk Import Craft Elements with CSV",
  "type": "craft-plugin",
  "license": "mit",
  "version": "1.1.0",
  "support": {
    "email": "samuelreichor@gmail.com",
    "issues": "https://github.com/samuelreichor/craft-genesis/issues?state=open",
    "source": "https://github.com/samuelreichor/craft-genesis",
    "docs": "https://github.com/samuelreichor/craft-genesis",
    "rss": "https://github.com/samuelreichor/craft-genesis/releases.atom"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.0.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "samuelreichor\\genesis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "samuelreichor\\genesis\\tests\\": "tests/"
    }
  },
  "extra": {
    "handle": "genesis",
    "name": "Genesis",
    "developer": "Samuel Reich√∂r",
    "documentationUrl": "https://github.com/samuelreichor/craft-genesis",
    "class": "samuelreichor\\genesis\\Genesis"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G",
    "test": "phpunit",
    "test:filter": "phpunit --filter"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
